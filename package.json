{
  "name": "web-app",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm:cms:server\" \"npm:app:only\"",
    "build": "vite build",
    "preview": "vite preview",
    "start": "vite preview --port 3000 --host",
    "start:simple": "npm run dev",
    "cms:server": "node --max-old-space-size=4096 mcp-cms/server.cjs",
    "cms:production": "node mcp-cms/server.cjs",
    "app:only": "node --max-old-space-size=4096 ./node_modules/.bin/vite",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext .js,.jsx --fix"
  },
  "dependencies": {
